os.loadAPI("/disk/apis/diag")
diag.drawDialog("NexEngine APIs",colors.cyan)
write("Uninstalling APIs")
diag.nextLine()
diag.nextLine()
write("> Removing [crypto]")
diag.nextLine()
write("> Removing [diag]")
diag.nextLine()
write("> Removing API directory")
fs.delete("/apis/")
diag.nextLine()
diag.nextLine()
write("APIs successfully uninstalled.")
sleep(2)
diag.clearScreen()